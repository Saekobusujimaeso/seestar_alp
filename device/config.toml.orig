title = "Alpaca Sample Driver (Rotator)"

[network]
# ip_address should not have to be changed
ip_address = "127.0.0.1"           # Any address
port = 5555
stport = 8090      #stellarium port
sthost = "172.18.160.1"  #stellarium hostname or IP
imgport = 7556
timeout = 5
rtsp_udp = true

[webui_settings]
uiport = 5432
# light/dark theme
uitheme = "dark" #all lower case
twilighttimes = true #true/false
experimental = true
confirm = false	# Enable/Disable the Commands page confirmation dialog
clear_sky_img_src = 'https://www.cleardarksky.com/c/LvrmrCAcsk.gif?c=1969222'
clear_sky_href = 'https://www.cleardarksky.com/c/LvrmrCAkey.html'
save_frames = false
save_frames_dir = "."
loading_gif = "loading.gif"

[server]
location = "Anywhere on Earth"  # Anything you want here
verbose_driver_exceptions = true

[device]
can_reverse = true
step_size = 1.0
steps_per_sec = 6

[seestar_initialization]
save_good_frames = true
save_all_frames = true              # including good and bad frames
#lat = 37.66                         # telescope location (lat), set to 0 if you want to guess from IP address
#long = -121.8                       # telescope location (long), set to 0 if you want to guess from IP address
lat = 34.1142994                    # parents home
long = -118.1748

gain = 100
exposure_length_preview_ms = 500
exposure_length_stack_ms = 10000
dither_enabled = true
dither_length_pixel = 50
dither_frequency = 30
activate_LP_filter = false                   
dew_heater_power = 30                # 0 - 100; 0 is off, 100 is max
scope_aim_lat = 70.0          # start up latitude in degrees -90 to 80
scope_aim_lon = 90.0          # start up longitude in degrees 0 to 360
is_EQ_mode = true

[logging]
log_level = "INFO"   #'WARN' only if trouble    #'INFO' to output more logging      #'DEBUG' still more output
log_prefix = ""
log_to_stdout = false
max_size_mb = 5
num_keep_logs = 10
log_events_in_info = true

[[seestars]]
name = "Seestar Alpha"
ip_address = "192.168.110.30"
device_num = 1
# You can specify the following settings to override those found in the seestar_initialization section on a per-device level
#scope_aim_lat = 60          # start up latitude in degrees -90 to 80 (override default)
#scope_aim_lon = 20          # start up longitude in degrees 0 to 360 (override default)
#is_EQ_mode = false          # can be used to override default

[[seestars]]
name = "Seestar Beta"
ip_address = "192.168.110.36"
device_num = 2

[[seestars]]
name = "Seestar Gamma"
ip_address = "192.168.110.238"
device_num = 3

[[seestars]]
name = "Seestar Delta"
ip_address = "192.168.1.133"
device_num = 4
